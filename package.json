    "axios": "^1.6.7",
    "dayjs": "^1.11.10",
    "framer-motion": "^11.0.3",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.12",
    "react-helmet-async": "^2.0.4",
